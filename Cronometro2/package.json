{
  "name": "cronometro2",
  "version": "2.1.2",
  "description": "Cronómetro con registro de marcas y tiempos",
  "productName": "Cronómetro",
  "author": "Leonardo Núñez <jorgebike.75@hotmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build -m capacitor -T android",
    "generar-apk": "npm run build && npm run cap:sync && npm run abrir-android",
    "abrir-android": "cd src-capacitor && npx cap open android",
    "cap:sync": "cd src-capacitor && npx cap sync android",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@capacitor-community/admob": "^7.2.0",
    "@capacitor/android": "^7.4.4",
    "@capacitor/preferences": "^7.0.2",
    "@quasar/extras": "^1.16.4",
    "@tabler/icons-vue": "^3.35.0",
    "@tabler/icons-webfont": "^3.35.0",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
